{
  "extends": "eslint:recommended",
  "ecmaFeatures": {
    "modules": true,
    "jsx": true
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "rules": {
    "no-undef": 2,
    "no-unused-vars": 2,
    "comma-dangle": 0,
    "no-console": 0,
    "semi": [2, "always"]
  },
  "plugins": [
    "react"
  ]
}
